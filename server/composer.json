{
    "name": "florianlechat/raven-url-shortener",
    "type": "project",
    "homepage": "https://github.com/FlorianLeChat/Raven-Url-Shortener",
    "license": "MIT",
    "authors": [
        {
            "name": "Florian Trayon",
            "homepage": "https://github.com/FlorianLeChat",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/FlorianLeChat/Raven-Url-Shortener/issues"
    },
    "prefer-stable": true,
    "require": {
        "php": ">=8.2",

        "ext-ctype": "*",
        "ext-iconv": "*",

        "doctrine/orm": "3.5.*",
        "doctrine/dbal": "4.*",
        "doctrine/annotations": "2.0.*",
        "doctrine/doctrine-bundle": "2.15.*",
        "doctrine/doctrine-migrations-bundle": "3.4.*",

        "nelmio/api-doc-bundle": "5.4.*",

        "symfony/uid": "7.3.*",
        "symfony/flex": "2.8.*",
        "symfony/lock": "7.3.*",
        "symfony/mailer": "7.3.*",
        "symfony/dotenv": "7.3.*",
        "symfony/console": "7.3.*",
        "symfony/runtime": "7.3.*",
        "symfony/validator": "7.3.*",
        "symfony/translation": "7.3.*",
        "symfony/http-client": "7.3.*",
        "symfony/rate-limiter": "7.3.*",
        "symfony/monolog-bundle": "3.10.*",
        "symfony/framework-bundle": "7.3.*"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "symfony/flex": true,
            "symfony/runtime": true,
            "php-http/discovery": true,
            "phpstan/extension-installer": true
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "require": "7.3.*",
            "allow-contrib": false
        }
    },
    "require-dev": {
        "phpmd/phpmd": "2.15.*",
        "phpunit/phpunit": "11.5.*",
        "phpstan/phpstan": "1.12.*",
        "phpstan/phpstan-symfony": "1.4.*",
        "phpstan/phpstan-doctrine": "1.5.*",
        "phpstan/extension-installer": "1.4.*",

        "symfony/process": "7.3.*",
        "symfony/browser-kit": "7.3.*",
        "symfony/debug-bundle": "7.3.*",

        "squizlabs/php_codesniffer": "3.13.*",
        "doctrine/doctrine-fixtures-bundle": "4.1.*"
    }
}
